<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Client" table="clients" schema="mms_schema" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="firstname">
            <column name="firstname" sql-type="text" not-null="true"/>
        </property>
        <property name="lastname">
            <column name="lastname" sql-type="text" not-null="true"/>
        </property>
        <property name="birthDate">
            <column name="birth_date" sql-type="date" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="text" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="text" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>